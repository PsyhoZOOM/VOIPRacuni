<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171"
  fx:controller="net.yuvideo.voipRacuni.Controllers.ExcelExportController">
  <children>
    <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
      AnchorPane.topAnchor="0.0" alignment="CENTER" prefHeight="26.0" prefWidth="260.0"
      spacing="20.0">
      <children>
        <DatePicker fx:id="dtpDatum"/>
        <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
          onAction="#exportCSV" text="Export" fx:id="bExport"/>
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
    </HBox>
  </children>
</AnchorPane>
